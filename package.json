{
  "name": "@lcap/mobile-ui",
  "version": "0.8.3",
  "lcapname": "@lcap/mobile-ui",
  "lcapversion": "0.6.3",
  "description": "Mobile UI Components built on Vue",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/index.css",
  "typings": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "types",
    "vetur"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev": "cross-env scecn=desktop vant-cli dev",
    "dev2": "vant-cli dev",
    "lint": "vant-cli lint",
    "test": "vant-cli test",
    "build2": "vant-cli build",
    "build": "cross-env scecn=desktop vant-cli build",
    "release": "vant-cli release",
    "test:watch": "vant-cli test --watch",
    "release:site": "sh docs/site/release.sh",
    "test:coverage": "open test/coverage/index.html",
    "move-yaml": "node bin/move-yaml",
    "move-docs": "node bin/move-docs",
    "screenshot": "node bin/screenshot",
    "deploy:dev": "node ./scripts/lcap && lcap deploy lib --platform http://lcp.vusion.top && node bin/deploy-lcp-images --platform=http://lcp.vusion.top",
    "deploy:dev2": "node ./scripts/lcap && node ./bin/upload.js",
    "deploy:test": "node ./scripts/lcap && lcap deploy lib --platform http://defaulttenant.low-code-fe.qa-ci-lowcode.service.163.org && node bin/deploy-lcp-images --platform=http://defaulttenant.low-code-fe.qa-ci-lowcode.service.163.org",
    "deploy:online": "node ./scripts/lcap && vusion deploy --framework cloud-ui.vusion@0.11 -v minor && vusion deploy --framework cloud-ui dist-theme && node bin/deploy-screenshots && node bin/deploy-drawings"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "vant-cli commit-lint"
    }
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{ts,tsx,js,vue,less}": "prettier --write",
    "*.{ts,tsx,js,vue}": "eslint --fix",
    "*.{vue,css,less}": "stylelint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:youzan/vant.git"
  },
  "keywords": [
    "ui",
    "vue",
    "frontend",
    "mobile ui",
    "component",
    "components"
  ],
  "author": "youzanfe",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.x",
    "@vant/icons": "^1.5.3",
    "@vant/popperjs": "^1.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "vue-lazyload": "1.2.3"
  },
  "peerDependencies": {
    "vue": ">= 2.6.0"
  },
  "devDependencies": {
    "@vant/area-data": "^1.1.1",
    "@vant/cli": "^2.10.3",
    "@vant/eslint-config": "^3.3.2",
    "@vue/compiler-sfc": "^3.2.2",
    "@xgheaven/nos-node-sdk": "^0.2.5",
    "cross-env": "^7.0.3",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "2.1.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vusion": "^0.12.18",
    "vusion-api": "^0.7.36",
    "yaml": "^1.10.2"
  },
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.css",
    "*.less"
  ],
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "web-types": "vetur/web-types.json",
  "eslintConfig": {
    "root": true,
    "extends": [
      "@vant"
    ]
  },
  "stylelint": {
    "extends": [
      "@vant/stylelint-config"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "proseWrap": "never"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
